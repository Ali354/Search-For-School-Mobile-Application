<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageButton
        android:id="@+id/Address"
        android:layout_width="65dp"
        android:layout_height="60dp"
        android:layout_marginStart="76dp"
        android:layout_marginLeft="76dp"
        android:layout_marginTop="196dp"
        android:background="@drawable/circlepng"
        android:scaleType="fitCenter"
        android:src="@drawable/address_icon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginLeft="4dp"
        android:layout_marginTop="12dp"
        android:text="Address"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/Address"
        app:layout_constraintTop_toBottomOf="@+id/Address" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginLeft="4dp"
        android:layout_marginTop="4dp"
        android:text="Professor"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/Professor"
        app:layout_constraintTop_toBottomOf="@+id/Professor" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:text="Student"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/Student"
        app:layout_constraintTop_toBottomOf="@+id/Student" />

    <ImageButton
        android:id="@+id/Professor"
        android:layout_width="71dp"
        android:layout_height="59dp"
        android:layout_marginStart="76dp"
        android:layout_marginLeft="76dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/circlepng"
        android:scaleType="fitCenter"
        android:src="@drawable/professor_icon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <ImageButton
        android:id="@+id/School"
        android:layout_width="66dp"
        android:layout_height="59dp"
        android:layout_marginTop="196dp"
        android:layout_marginEnd="52dp"
        android:layout_marginRight="52dp"
        android:background="@drawable/circlepng"
        android:scaleType="fitCenter"
        android:src="@drawable/school_icon"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/Student"
        android:layout_width="69dp"
        android:layout_height="57dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="52dp"
        android:layout_marginRight="52dp"
        android:background="@drawable/circlepng"
        android:scaleType="fitCenter"
        android:src="@drawable/student_icon_removebg_preview"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:text="School"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/School"
        app:layout_constraintTop_toBottomOf="@+id/School" />

</androidx.constraintlayout.widget.ConstraintLayout>